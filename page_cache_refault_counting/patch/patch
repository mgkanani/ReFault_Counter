+struct my_rq {
+        spinlock_t          lock;   /* spin lock that protects this runqueue */
+        unsigned long       nr_running;         /* number of runnable tasks */
+        unsigned long       nr_switches;        /* context switch count */
+        unsigned long       expired_timestamp;    /* time of last array swap */
+        unsigned long       nr_uninterruptible;   /* uninterruptible tasks */
+        unsigned long long  timestamp_last_tick;  /* last scheduler tick */
+        struct task_struct  *curr;                /* currently running task */
+        struct task_struct  *idle;           /* this processor's idle task */
+        struct mm_struct    *prev_mm;        /* mm_struct of last ran task */
+        struct task_struct  *migration_thread; /* migration thread */
+        struct list_head    migration_queue;   /* migration queue*/
+        atomic_t            nr_iowait; /* number of tasks waiting on I/O */
+	int		    cpu;
+	unsigned long long  now;
+};
+
+void copy_to_my_rq(struct rq *rq, struct my_rq *my_rq){
+	my_rq->nr_running = rq->nr_running ;
+	my_rq->nr_switches = rq->nr_switches ;
+//	my_rq->expired_timestamp = rq->expired_timestamp ;
+	my_rq->nr_uninterruptible = rq->nr_uninterruptible ;
+//	my_rq->timestamp_last_tick = rq->timestamp_last_tick ;
+	my_rq->nr_iowait = rq->nr_iowait ;
+	my_rq->cpu = rq->cpu;
+	my_rq->now = rq_clock(rq);
+}
+
+
+static inline void my_calc_cputime(struct my_rq *rq, struct task_struct *tsk){
+}
+
+static void (*p_calc_cputime)(struct my_rq *rq, struct task_struct *tsk)=&my_calc_cputime;
+EXPORT_SYMBOL(p_calc_cputime);
+
+
+static void my_parse_func(struct rq *rq, struct task_struct *tsk){
+	struct my_rq my_rq;
+	if(p_calc_cputime!=&my_calc_cputime){
+	//module has been loaded so inititialize my_rq and call it.
+		copy_to_my_rq(rq,&my_rq);
+		p_calc_cputime(&my_rq,tsk);
+	}
+/*	else{//nothing to do
+		p_calc_cputime(rq,prev);
+	}
+*/
+}
