+#include<linux/kernel.h>
+
+long def_hello(void){
+	printk("Hello System call called.\n");
+	return 0;
+}
+static long (*ref_hello)(void)=&def_hello;
+EXPORT_SYMBOL(ref_hello);
+asmlinkage long sys_hello(void){
+	return (ref_hello!=&def_hello)?ref_hello():def_hello();
+}
